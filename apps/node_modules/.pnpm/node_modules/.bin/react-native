#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ander/Descargas/HackMoney2026/apps/node_modules/.pnpm/react-native@0.83.1_@babel+core@7.29.0_@types+react@19.2.11_bufferutil@4.1.0_react@19.2.4_utf-8-validate@5.0.10/node_modules/react-native/node_modules:/home/ander/Descargas/HackMoney2026/apps/node_modules/.pnpm/react-native@0.83.1_@babel+core@7.29.0_@types+react@19.2.11_bufferutil@4.1.0_react@19.2.4_utf-8-validate@5.0.10/node_modules:/home/ander/Descargas/HackMoney2026/apps/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/ander/Descargas/HackMoney2026/apps/node_modules/.pnpm/react-native@0.83.1_@babel+core@7.29.0_@types+react@19.2.11_bufferutil@4.1.0_react@19.2.4_utf-8-validate@5.0.10/node_modules/react-native/node_modules:/home/ander/Descargas/HackMoney2026/apps/node_modules/.pnpm/react-native@0.83.1_@babel+core@7.29.0_@types+react@19.2.11_bufferutil@4.1.0_react@19.2.4_utf-8-validate@5.0.10/node_modules:/home/ander/Descargas/HackMoney2026/apps/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../react-native/cli.js" "$@"
else
  exec node  "$basedir/../react-native/cli.js" "$@"
fi
